{
    "compilerOptions": {
        "outDir":"./dist",
        "module": "CommonJs",
        "target": "es2016",
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks":true,
        "allowJs":true,
        "resolveJsonModule": true,
        "esModuleInterop":true,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,



    },
    "include": [
        "./src/**/*"
    ]
}